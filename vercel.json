{
  "version": 2,
  "buildCommand": "vite build",
  "outputDirectory": "dist/public",
  "functions": {
    "api/[...route].ts": {
      "runtime": "@vercel/node@3.2.18"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/[...route]"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
