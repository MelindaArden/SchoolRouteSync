{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "distDir": "dist/public"
      }
    },
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/login.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/session.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/mobile-login.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/users/[id].js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/login",
      "dest": "/api/login.js"
    },
    {
      "src": "/api/session",
      "dest": "/api/session.js"
    },
    {
      "src": "/api/mobile-login",
      "dest": "/api/mobile-login.js"
    },
    {
      "src": "/api/users/([^/]+)",
      "dest": "/api/users/[id].js?id=$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
